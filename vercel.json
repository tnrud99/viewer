{
  "version": 2,
  "builds": [
    {
      "src": "src/server/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "src/server/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "src/editor/**",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/server/server.js"
    },
    {
      "src": "/js/(.*)",
      "dest": "src/server/public/js/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "src/server/public/css/$1"
    },
    {
      "src": "/viewer.html",
      "dest": "src/server/public/viewer.html"
    },
    {
      "src": "/src/editor/(.*)",
      "dest": "src/editor/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "assets/$1"
    },
    {
      "src": "/timestamp_1751179878134.json",
      "dest": "assets/samples/timestamp_1751179878134.json"
    },
    {
      "src": "/(.*)",
      "dest": "src/server/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "development": true,
      "production": true,
      "homepage": true
    }
  },
  "headers": [
    {
      "source": "/homepage image/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|webp))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(html|json))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
} 